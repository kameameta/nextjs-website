{
  "name": "nextjs-example-eslint-ts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "stylelint": "stylelint '**/*.css'",
    "prettier": "prettier --check **/*.{js,ts,tsx,css}",
    "lint": "npm run prettier && npm run eslint && npm run stylelint && npm run type-check"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/styled-components": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-next": "^12.0.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.2"
  }
}
