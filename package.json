{
  "name": "nextjs-example-eslint-ts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "stylelint": "stylelint '**/*.css'",
    "prettier": "prettier --check **/*.{js,ts,tsx,css}",
    "lint": "npm run prettier && npm run eslint && npm run stylelint && npm run type-check"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.5.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.3"
  }
}
